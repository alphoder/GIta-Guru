# Anthropic API Key (required only if using Claude as LLM provider)
ANTHROPIC_API_KEY=your_anthropic_key_here

# OpenAI API Key (required only if using OpenAI as LLM provider)
OPENAI_API_KEY=your_openai_key_here

# LLM Provider: "ollama", "anthropic", or "openai"
LLM_PROVIDER=openai

# OpenAI model (optional, defaults to gpt-4o)
OPENAI_MODEL=gpt-4o
